# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Yelp API Configuration
YELP_API_KEY=your_yelp_api_key_here

# API Gateway Configuration
API_GATEWAY_URL=https://your-api-id.execute-api.us-east-1.amazonaws.com/prod/v1

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=yelp-restaurants

# SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/restaurant-requests
SQS_DLQ_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/restaurant-requests-dlq

# SES Configuration
SES_FROM_EMAIL=noreply@yourdomain.com
SES_REGION=us-east-1

# OpenSearch Configuration
OPENSEARCH_DOMAIN=restaurant-search
OPENSEARCH_ENDPOINT=https://search-restaurant-search-xxxxx.us-east-1.es.amazonaws.com
OPENSEARCH_INDEX=restaurants

# Lex Configuration
LEX_BOT_NAME=DiningConciergeBot
LEX_BOT_ALIAS=PROD

# Frontend Configuration
FRONTEND_BUCKET_NAME=your-frontend-bucket-name
FRONTEND_REGION=us-east-1
